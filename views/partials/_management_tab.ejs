<!-- MANAGEMENT TAB -->
<section id="management" class="tab-content p-6 min-h-0 overflow-y-auto hidden">
  <div class="max-w-6xl mx-auto space-y-8">
    <!-- Header -->
    <div class="border-b border-zinc-800 pb-4">
